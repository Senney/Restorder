<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Restorder.OrderBillControl"
	x:Name="UserControl"
	d:DesignWidth="233.5" d:DesignHeight="573.833">

	<Grid x:Name="LayoutRoot">
		<ScrollViewer x:Name="OrderBill" UseLayoutRounding="True" ScrollViewer.CanContentScroll="True" ScrollViewer.VerticalScrollBarVisibility="Hidden" Margin="-2,0,-0.5,-0.167" d:LayoutOverrides="HorizontalMargin">
			<Grid x:Name="BillGrid" Height="574" Width="236">
				<TextBlock x:Name="Seat" Height="40" TextWrapping="Wrap" VerticalAlignment="Top" Margin="8,0,93,0" Text="Seat Number"/>
				<TextBlock x:Name="SubTotal" Height="40" TextWrapping="Wrap" Text="Subtotal" VerticalAlignment="Top" HorizontalAlignment="Right" Width="89"/>
				<StackPanel x:Name="ItemList" Margin="2,44,0,0"/>
			</Grid>
		</ScrollViewer>
	</Grid>
</UserControl>