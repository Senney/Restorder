	xmlns:local="clr-namespace:Restorder"
	mc:Ignorable="d"
	x:Class="Restorder.MenuExpander"
	x:Name="UserControl"
	d:DesignWidth="512.5" d:DesignHeight="84">

	<Grid x:Name="LayoutRoot">
		<Expander x:Name="Section">
            <StackPanel x:Name="Children" Margin="34,0,6,0">
                
